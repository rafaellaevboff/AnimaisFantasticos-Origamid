{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-tabs": 0,
        "no-param-reassign": 0,
        "import/extensions": 0,
        "no-console": 0,
        "linebreak-style": ["error" , "windows"],
        "indent": ["error", 4]
    }
}